{
  "name": "@agentid/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for AgentID - Identity and authorization for AI agents",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./express": {
      "import": "./dist/middleware/express.mjs",
      "require": "./dist/middleware/express.js",
      "types": "./dist/middleware/express.d.ts"
    },
    "./verifier": {
      "import": "./dist/verifier.mjs",
      "require": "./dist/verifier.js",
      "types": "./dist/verifier.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "identity",
    "credentials",
    "authorization",
    "authentication",
    "sdk"
  ],
  "author": "AgentID <sdk@agentid.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentid/agentid-js"
  },
  "homepage": "https://agentid.dev",
  "bugs": {
    "url": "https://github.com/agentid/agentid-js/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "express": "^4.18.2",
    "msw": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}
