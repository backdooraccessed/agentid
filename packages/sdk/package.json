{
  "name": "@agentid/sdk",
  "version": "0.1.0",
  "description": "JavaScript SDK for verifying AgentID credentials",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@noble/ed25519": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "agentid",
    "ai-agents",
    "credentials",
    "verification",
    "ed25519"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/agentid/agentid"
  },
  "license": "MIT"
}
